<smil>
	<head>
		<meta http-equiv="refresh" content="20"/>
		<layout>
			<!-- define the screen resolution -->
			<root-layout width="1080" height="1920" backgroundColor="#18182c" />

			<!-- define the screen layout, in this case single fullscreen zone -->
			<region regionName="main" left="0" top="0" width="1920" height="1080" z-index="1" backgroundColor="#18182c"/>
		</layout>
	</head>
	<body>
		<par>

			<!-- Downloading resources into the internal storage -->
			<seq>
				<prefetch src="https://static.signageos.io/assets/test-videos-02_AME/video-test-02_15s_2160x3840_8bfc28d64e4e9e4e8d903e1864d93ef9.mp4" />
				<prefetch src="https://static.signageos.io/assets/test-videos-03_AME/video-test-03_15s_2160x3840_d992c3f6f0b5364c2b8340fa0e33cede.mp4" />
				<prefetch src="https://static.signageos.io/assets/test-videos-04_AME/video-test-04_15s_2160x3840_3f2cb02a99078e572bd7eeca575eac1b.mp4" />
				<prefetch src="https://static.signageos.io/assets/test-videos-05_AME/video-test-05_15s_2160x3840_984597d95517004a9befb2425719f651.mp4" />
				<prefetch src="https://static.signageos.io/assets/test-videos-06_AME/video-test-06_15s_2160x3840_a3d45a9cce6333646433f35d9559f022.mp4" />

				<seq id="__prefetchEnd" dur="1s" />
			</seq>

			<!-- Wait for the prefetch of media files, once done, start the playback -->
			<par begin="__prefetchEnd.endEvent" repeatCount="indefinite">

				<!-- indefinite loop of media files in the selected order -->
				<seq repeatCount="indefinite">
					<video src="https://static.signageos.io/assets/test-videos-02_AME/video-test-02_15s_2160x3840_8bfc28d64e4e9e4e8d903e1864d93ef9.mp4" region="main" soundLevel="0%"></video>
					<video src="https://static.signageos.io/assets/test-videos-04_AME/video-test-04_15s_2160x3840_3f2cb02a99078e572bd7eeca575eac1b.mp4" region="main" soundLevel="0%"></video>
					<video src="https://static.signageos.io/assets/test-videos-03_AME/video-test-03_15s_2160x3840_d992c3f6f0b5364c2b8340fa0e33cede.mp4" region="main" soundLevel="0%"></video>
					<video src="https://static.signageos.io/assets/test-videos-05_AME/video-test-05_15s_2160x3840_984597d95517004a9befb2425719f651.mp4" region="main" soundLevel="0%"></video>
					<video src="https://static.signageos.io/assets/test-videos-06_AME/video-test-06_15s_2160x3840_a3d45a9cce6333646433f35d9559f022.mp4" region="main" soundLevel="0%"></video>
				</seq>
			</par>
		</par>
	</body>
</smil>